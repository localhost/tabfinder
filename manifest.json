{
  "name": "TabFinder",
  "version": "0.1.0",
  "description": "Find your tabs",
  "background_page": "background.html",
  "browser_action": {
    "default_title": "TabFinder",
    "default_icon": "gfx/icon-48.png",
    "popup": "popup.html"
  },
  "icons": {
    "16": "gfx/icon-16.png",
    "48": "gfx/icon-48.png",
    "128": "gfx/icon-128.png"
  },
  "permissions": [ "tabs" ],
  "minimum_chrome_version": "5.0.316.0" /* chrome.browserAction.setPopup */
}
